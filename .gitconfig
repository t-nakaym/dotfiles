[user]
	name = t-nakaym
	email = tnakaym@gmail.com
[core]
	pager = lv -c
	excludesfile = /home/t-nakaym/.global_gitignore
[color]
	status = auto
	diff = auto
	branch = auto
	interactive = auto
	grep = auto
	ui = true
[alias]
	st = status
	ci = commit
	co = checkout
	l = log --pretty=format:\"%ci %C(yellow)%H%Creset [%cn] %Cgreen%s %C(cyan)%d%Creset\" -10
	lg = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
	b = branch
	d = diff
	dc = diff --cached
